{"swagger":"2.0","info":{"title":"LibraryApp3","version":"1.0.0"},"paths":{"/book":{"post":{"summary":"CreateBook creates a book in the library. We do not de-duplicate the requests as this is the tutorial API.","operationId":"BookService_CreateBook","parameters":[{"name":"body","in":"body","required":true,"schema":{"$ref":"#/definitions/library_appBook"}}],"responses":{"200":{"description":"A successful response.","schema":{"$ref":"#/definitions/library_appBook"}},"default":{"description":"An unexpected error response.","schema":{"$ref":"#/definitions/rpcStatus"}}},"tags":["BookService"]}},"/book/{isbn}":{"delete":{"summary":"DeleteBook deletes the book from the library. Warning! This action cannot be reverted and doesn't require any confirmation.","operationId":"BookService_DeleteBook","parameters":[{"name":"isbn","in":"path","required":true,"type":"string"},{"name":"title","in":"query","type":"string"}],"responses":{"200":{"description":"A successful response.","schema":{"$ref":"#/definitions/library_appBook"}},"default":{"description":"An unexpected error response.","schema":{"$ref":"#/definitions/rpcStatus"}}},"tags":["BookService"]}},"/books":{"get":{"summary":"ListBooks returns the list of books available in the library without any pagination.","operationId":"BookService_ListBooks","responses":{"200":{"description":"A successful response.","schema":{"$ref":"#/definitions/library_appListBookResponse"}},"default":{"description":"An unexpected error response.","schema":{"$ref":"#/definitions/rpcStatus"}}},"tags":["BookService"]}}},"host":"","definitions":{"library_appAuthor":{"type":"object","properties":{"firstName":{"type":"string"},"lastName":{"type":"string"}},"example":{"first_name":"Stephen","last_name":"King"}},"library_appBook":{"type":"object","properties":{"author":{"$ref":"#/definitions/library_appAuthor"},"isbn":{"type":"string"},"title":{"type":"string"}},"title":"The simplified structure represents a single book.\n","example":{"author":{"first_name"